{
	"name": "11ty-portfolio",
	"version": "1.0.0",
	"description": "Minimal Poftfolio / blog template built with eleventy & tailwindcss.",
	"scripts": {
		"start": "npm run dev",
		"dev": "NODE_ENV=development run-p clean css eleventy browsersync",
		"build": "NODE_ENV=production clean && eleventy && tailwindcss -i src/assets/styles/tailwind.css -o public/assets/styles/style.css",
		"clean": "del-cli public",
		"css": "postcss src/assets/styles/tailwind.css --o public/assets/styles/styles.css --watch",
		"eleventy": "eleventy --watch",
		"browsersync": "browser-sync start --server 'public' --files 'public' --port 8080 --no-notify --no-open"
	},
	"keywords": [
		"11ty",
		"tailwindcss",
		"eleventy"
	],
	"author": "kailoon",
	"license": "ISC",
	"devDependencies": {
		"@11ty/eleventy": "^1.0.1",
		"@11ty/eleventy-navigation": "^0.3.3",
		"@11ty/eleventy-plugin-rss": "^1.1.2",
		"@11ty/eleventy-plugin-syntaxhighlight": "^4.0.0",
		"autoprefixer": "^10.4.7",
		"browser-sync": "^2.27.10",
		"cssnano": "^5.1.9",
		"del-cli": "^4.0.1",
		"luxon": "^2.4.0",
		"markdown-it": "^13.0.1",
		"markdown-it-anchor": "^8.6.4",
		"markdown-it-link-attributes": "^4.0.0",
		"npm-run-all": "^4.1.5",
		"nunjucks": "^3.2.3",
		"postcss-cli": "^9.1.0",
		"tailwindcss": "^3.0.24"
	}
}
